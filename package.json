{
  "name": "men-restapi-s24",
  "version": "1.0.0",
  "description": "MongoDB-ExpressJS-NodeJS REST API",
  "main": "server.js",
  "scripts": {
    "start-dev": "cross-env NODE_ENV=development nodemon server.js",
    "test": "cross-env NODE_ENV=test mocha --exit"
  },
  "nodemonConfig": {
    "verbose": false,
    "watch": [
      ".env",
      "*.js",
      "*.yaml"
    ],
    "ignore": [
      "node_modules",
      "swagger-output.json"
    ]
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "node",
    "express"
  ],
  "author": "smsj",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.0.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^6.1.7",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chai-http": "^4.4.0",
    "mocha": "^10.4.0"
  }
}
